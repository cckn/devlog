{
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": true
  },
  "files.exclude": {
    "node_modules": true,
    "static": true,
    ".gitignore": true,
    ".github": true,
    ".cache": true,
    "assets": true,
    ".eslintrc.js": true,
    ".markdownlint.json": true,
    ".prettierrc": true,
    "CONTRIBUTING.md": true,
    "LICENSE": true,
    "package-lock.json": true,
    "README.md": true,
    "SECURITY.md": true,
    ".vscode": true
  },
  "editor.formatOnSave": true,
  "typewriterAutoScroll.enable": false
}
